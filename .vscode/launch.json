{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "launch_debug",
			"type": "cppdbg",
			"request": "launch",
			"program": "${workspaceFolder}/example.out",
			"args": [],
			"stopAtEntry": false,
			"cwd": "${workspaceFolder}",
			"environment": [],
			"externalConsole": false,
			"MIMode": "gdb",
			"setupCommands": [
				{
					"description": "Enable pretty-printing for gdb",
					"text": "-enable-pretty-printing"
				}
			],
			"preLaunchTask": "make_debug",
			"miDebuggerPath": "/usr/bin/gdb"
		}
	]
}